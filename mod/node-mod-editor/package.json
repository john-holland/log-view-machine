{
  "name": "node-mod-editor",
  "version": "1.0.0",
  "type": "module",
  "description": "Mod Editor Node.js Backend with OpenTelemetry and Unleash",
  "main": "src/main-server.js",
  "scripts": {
    "build": "npm run build --prefix frontend",
    "start": "node src/main-server.js",
    "dev": "nodemon src/main-server.js",
    "tome": "node src/tome-server.js",
    "dev:tome": "nodemon src/tome-server.js",
    "test": "jest --config jest.config.mjs",
    "test:watch": "jest --config jest.config.mjs --watch",
    "test:coverage": "jest --config jest.config.mjs --coverage",
    "test:unit": "jest --config jest.config.mjs --testPathPattern=__tests__",
    "test:pact": "jest --config jest.config.mjs --testPathPattern=modApiClient.pact",
    "test:editor-publish": "node scripts/test-editor-publish.mjs",
    "test:editor-auth": "jest --config jest.config.mjs --testPathPattern=can-publish && node scripts/test-editor-publish.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:shell": "./scripts/run-tests.sh",
    "test:e2e:shell:headed": "./scripts/run-tests.sh --headed",
    "test:e2e:shell:debug": "./scripts/run-tests.sh --debug",
    "test:e2e:shell:ui": "./scripts/run-tests.sh --ui",
    "test:e2e:shell:browser": "./scripts/run-tests.sh --browser",
    "test:all": "npm run test && npm run test:e2e",
    "test:install": "npx playwright install",
    "test:report": "npx playwright show-report",
    "compose:merge": "node scripts/merge-compose-fragments.js",
    "lint": "eslint src/",
    "docker:build": "docker build -t node-mod-editor .",
    "docker:run": "docker run -p 3001:3001 node-mod-editor"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/instrumentation-express": "^0.33.0",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-trace-base": "^1.21.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@opentelemetry/semantic-conventions": "^1.21.0",
    "@xstate/react": "^4.0.0",
    "ace-builds": "^1.43.6",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "continuum-cave-adapter": "file:../../packages/continuum-cave-adapter",
    "sharepoint-cave-adapter": "file:../../packages/sharepoint-cave-adapter",
    "cors": "^2.8.5",
    "docker-cavestartup-adapter": "file:../../packages/docker-cavestartup-adapter",
    "dotcms-cavemodloader-adapter": "file:../../packages/dotcms-cavemodloader-adapter",
    "dotcms-login-adapter": "file:../../packages/dotcms-login-adapter",
    "dotcms-pam-cave-adapter": "file:../../packages/dotcms-pam-cave-adapter",
    "dotcms-startup-adapter": "file:../../packages/dotcms-startup-adapter",
    "dotenv": "^17.2.1",
    "duckdb-cavedb-adapter": "file:../../packages/duckdb-cavedb-adapter",
    "email-send-adapter": "file:../../packages/email-send-adapter",
    "express": "^4.18.2",
    "express-cave-adapter": "file:../../packages/express-cave-adapter",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "genericeditor-cavemod-adapter": "file:../../packages/genericeditor-cavemod-adapter",
    "google-auth-library": "^10.5.0",
    "google-login-adapter": "file:../../packages/google-login-adapter",
    "graphql-ws": "^6.0.6",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "log-view-machine": "file:../../log-view-machine",
    "login-handler-adapter": "file:../../packages/login-handler-adapter",
    "modload-eventedcavemodorder-adapter": "file:../../packages/modload-eventedcavemodorder-adapter",
    "morgan": "^1.10.0",
    "opentelemetry-cavemetrics-adapter": "file:../../packages/opentelemetry-cavemetrics-adapter",
    "opentelemetry-cavestartup-adapter": "file:../../packages/opentelemetry-cavestartup-adapter",
    "port-cavestartup-adapter": "file:../../packages/port-cavestartup-adapter",
    "pythonapp-caveservice-adapter": "file:../../packages/pythonapp-caveservice-adapter",
    "react": "^18.0.0",
    "security-adapter": "file:../../packages/security-adapter",
    "sqlite3": "^5.1.7",
    "suneditor": "^2.47.8",
    "unleash-cavestartup-adapter": "file:../../packages/unleash-cavestartup-adapter",
    "unleash-cavetoggles-adapter": "file:../../packages/unleash-cavetoggles-adapter",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@pact-foundation/pact": "^14.0.0",
    "@playwright/test": "^1.54.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "fish-burger",
    "xstate",
    "opentelemetry",
    "tracing",
    "unleash",
    "backend"
  ],
  "author": "Fish Burger Team",
  "license": "MIT"
}
